             mc:Ignorable="d" 
             Height="500" Width="800">
    <DockPanel Background="LightSlateGray">
        <DockPanel DockPanel.Dock="Top">
            <Button Width="50" Content="Tune" Margin="5" DockPanel.Dock="Right"/>
            <ComboBox ItemsSource="{Binding Source={utils:EnumBindingSource {x:Type enums:Genre}}}"
                      SelectedValue="{Binding SelectedGenre}"
                      Width="100" Margin="5" DockPanel.Dock="Right"/>
            <Separator Visibility="Hidden" DockPanel.Dock="Left"/>
        </DockPanel>
        <DockPanel DockPanel.Dock="Bottom" HorizontalAlignment="Center">
            <DockPanel DockPanel.Dock="Left" Width="200" Height="70">
                <DockPanel.LayoutTransform>
                    <RotateTransform Angle="-90"/>
                </DockPanel.LayoutTransform>
                <TextBlock Text="16 Hz" DockPanel.Dock="Left" VerticalAlignment="Center">
                                    <TextBlock.LayoutTransform>
                    <RotateTransform Angle="90"/>
                </TextBlock.LayoutTransform>
                </TextBlock>
                <Slider x:Name="Slider01"  DockPanel.Dock="Right" VerticalAlignment="Center" Minimum="-120" Maximum="0" Value="-50"/>
            </DockPanel>
            <DockPanel DockPanel.Dock="Left" Width="200" Height="70">
                <DockPanel.LayoutTransform>
                    <RotateTransform Angle="-90"/>
                </DockPanel.LayoutTransform>
